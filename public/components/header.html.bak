        </a>
        
        <!-- Bot√≥n Men√∫ Hamburguesa y Cambio de Idioma agrupados a la derecha -->
        <div class="flex items-center gap-4">
            <!-- Men√∫ desplegable de idiomas -->
            <div class="relative group">
                <button id="language-toggle" class="bg-black text-white font-bold py-1.5 md:py-2 px-3 md:px-4 rounded-md text-xs md:text-sm uppercase hover:bg-gray-800 transition-colors duration-200 flex items-center" onclick="event.stopPropagation(); document.querySelector('.language-dropdown').classList.toggle('hidden')">
                    <span class="whitespace-nowrap">IDIOMA</span>
                    <svg class="w-3 h-3 md:w-4 md:h-4 ml-1 md:ml-2 transition-transform duration-200" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"></path>
                    </svg>
                </button>
                
                <!-- Men√∫ desplegable -->
                <div class="language-dropdown absolute right-0 mt-2 w-40 bg-white rounded-md shadow-lg py-1 z-50 hidden">
                    <a href="#" class="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100" onclick="changeLanguage('es'); return false;">
                        <span class="flex items-center">
                            <span class="mr-2">üá™üá∏</span>
                            <span>Espa√±ol</span>
                            <span id="check-es" class="ml-auto text-green-500 hidden">‚úì</span>
                        </span>
                    </a>
                    <a href="#" class="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100" onclick="changeLanguage('en'); return false;">
                        <span class="flex items-center">
                            <span class="mr-2">üá¨üáß</span>
                            <span>English</span>
                            <span id="check-en" class="ml-auto text-green-500 hidden">‚úì</span>
                        </span>
                    </a>
                </div>
            </div>
            
            <!-- Google Translate Element (oculto) -->
            <div id="google_translate_element" style="display: none !important;"></div>
            
            <script>
            // Cargar el script del selector de idiomas
            (function() {
                const script = document.createElement('script');
                script.src = '/js/language-switcher.js';
                script.async = true;
                document.head.appendChild(script);
            })();

            // Inicializar el traductor
            function googleTranslateElementInit() {
                // Verificar si ya est√° inicializado
                if (window.googleTranslateInitialized) {
                    console.log('Google Translate ya est√° inicializado');
                    return;
                }
                
                try {
                    // Verificar si Google Translate est√° disponible
                    if (typeof google !== 'undefined' && google.translate && google.translate.TranslateElement) {
                        // Verificar si el elemento existe
                        const translateElement = document.getElementById('google_translate_element');
                        if (!translateElement) {
                            console.error('Elemento google_translate_element no encontrado');
                            return;
                        }
                        
                        // Inicializar Google Translate
                        new google.translate.TranslateElement({
                            pageLanguage: 'es',
                            includedLanguages: 'es,en',
                            layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                            autoDisplay: false
                        }, 'google_translate_element');
                        
                        console.log('Google Translate inicializado correctamente');
                        window.googleTranslateInitialized = true;
                        
                        // Ocultar elementos de Google Translate
                        hideGoogleElements();
                        // Verificar peri√≥dicamente para asegurarnos de que los elementos sigan ocultos
                        setInterval(hideGoogleElements, 1000);
                        
                        // Configurar idioma guardado
                        const savedLang = localStorage.getItem('userLanguage') || 'es';
                        const urlLang = new URLSearchParams(window.location.search).get('lang');
                        const langToSet = urlLang || savedLang;
                        
                        // Actualizar la interfaz de usuario
                        updateLanguageUI(langToSet);
                        
                        // Forzar el cambio de idioma despu√©s de un breve retraso
                        setTimeout(() => {
                            forceGoogleTranslateUpdate(langToSet) || updateLanguageUI(langToSet);
                        }, 500);
                        
                    } else {
                        console.error('Google Translate no est√° disponible');
                        // Intentar recargar si no est√° disponible
                        setTimeout(initializeTranslator, 1000);
                    }
                } catch (error) {
                    console.error('Error al inicializar Google Translate:', error);
                }
            }
            
            // Funci√≥n para ocultar elementos de Google Translate
            function hideGoogleElements() {
                const elements = document.querySelectorAll('.goog-te-gadget, .goog-te-banner-frame, .skiptranslate, .goog-te-gt-bb');
                elements.forEach(el => {
                    if (el) el.style.display = 'none';
                });
                
                const iframes = document.querySelectorAll('iframe[src*="translate"]');
                iframes.forEach(iframe => {
                    if (iframe && iframe.src.includes('translate')) {
                        iframe.style.display = 'none';
                    }
                });
            }
            
            // Funci√≥n global para cambiar el idioma (se definir√° despu√©s de cargar language-switcher.js)
            window.changeLanguage = function(lang) {
                if (window.languageSwitcher) {
                    window.languageSwitcher.changeLanguage(lang);
                } else {
                    // Fallback en caso de que el script no se haya cargado
                    localStorage.setItem('userLanguage', lang);
                    const url = new URL(window.location.href);
                    url.searchParams.set('lang', lang);
                    window.location.href = url.toString();
                }
                return false;
            };
            
            // Funci√≥n para actualizar la interfaz (ser√° manejada por language-switcher.js)
            function updateLanguageUI(lang) {
                // Esta funci√≥n ser√° reemplazada por la implementaci√≥n en language-switcher.js
                console.log('Actualizando interfaz al idioma:', lang);
                document.getElementById('check-es').classList.toggle('hidden', lang !== 'es');
                document.getElementById('check-en').classList.toggle('hidden', lang !== 'en');
            }
            
            // Cargar Google Translate con manejo de errores
            function loadGoogleTranslate() {
                return new Promise((resolve, reject) => {
                    // Verificar si ya est√° cargado
                    if (window.google && window.google.translate) {
                        console.log('Google Translate ya est√° cargado');
                        resolve();
                        return;
                    }
                    
                    // Eliminar cualquier script anterior
                    const oldScript = document.querySelector('script[src*="translate.google.com"]');
                    if (oldScript) {
                        oldScript.remove();
                    }
                    
                    const script = document.createElement('script');
                    script.src = 'https://translate.google.com/translate_a/element.js?cb=googleTranslateElementInit&hl=es';
                    script.async = true;
                    
                    script.onload = () => {
                        console.log('Google Translate cargado correctamente');
                        // Verificar que la API est√© disponible
                        if (window.google && window.google.translate) {
                            resolve();
                        } else {
                            reject(new Error('La API de Google Translate no est√° disponible'));
                        }
                    };
                    
                    script.onerror = (error) => {
                        console.error('Error al cargar Google Translate:', error);
                        reject(error);
                    };
                    
                    document.head.appendChild(script);
                });
            }
            
            // Funci√≥n para inicializar el traductor
            function initializeTranslator() {
                // Verificar si ya est√° inicializado
                if (window.initializingTranslator) {
                    console.log('El traductor ya se est√° inicializando');
                    return;
                }
                
                window.initializingTranslator = true;
                
                try {
                    // Verificar si ya hay un idioma guardado
                    const savedLang = localStorage.getItem('userLanguage');
                    const urlParams = new URLSearchParams(window.location.search);
                    const urlLang = urlParams.get('lang');
                    
                    // Priorizar el idioma de la URL sobre el guardado
                    const langToSet = urlLang || savedLang || 'es';
                    
                    // Actualizar el idioma en el almacenamiento local si es diferente
                    if (urlLang && urlLang !== savedLang) {
                        localStorage.setItem('userLanguage', urlLang);
                    } else if (!savedLang) {
                        localStorage.setItem('userLanguage', 'es');
                    }
                    
                    // Actualizar la interfaz de usuario inmediatamente
                    updateLanguageUI(langToSet);
                    
                    // Cargar Google Translate
                    loadGoogleTranslate()
                        .then(() => {
                            console.log('Inicializando Google Translate...');
                            // Si ya est√° definida, llamar directamente
                            if (typeof googleTranslateElementInit === 'function') {
                                googleTranslateElementInit();
                            }
                            
                            // Verificar despu√©s de un tiempo si se inicializ√≥ correctamente
                            setTimeout(() => {
                                if (!window.googleTranslateInitialized) {
                                    console.warn('Google Translate no se inicializ√≥ correctamente, reintentando...');
                                    initializeTranslator();
                                }
                            }, 2000);
                        })
                        .catch(error => {
                            console.error('No se pudo cargar Google Translate:', error);
                            // Mostrar un mensaje de error o intentar una soluci√≥n alternativa
                            updateLanguageUI(langToSet);
                            
                            // Reintentar despu√©s de un tiempo
                            setTimeout(initializeTranslator, 3000);
                        });
                } catch (error) {
                    console.error('Error en initializeTranslator:', error);
                } finally {
                    window.initializingTranslator = false;
                }
            }
            
            // Inicializaci√≥n del selector de idiomas (manejado por language-switcher.js)
            document.addEventListener('DOMContentLoaded', function() {
                // Funci√≥n para manejar clics en el bot√≥n de idioma
                const languageToggle = document.getElementById('language-toggle');
                if (languageToggle) {
                    languageToggle.addEventListener('click', function(e) {
                        e.stopPropagation();
                        const languageMenu = document.querySelector('.language-dropdown');
                        if (languageMenu) {
                            languageMenu.classList.toggle('hidden');
                        }
                    });
                }
                
                // Cerrar men√∫ al hacer clic fuera
                document.addEventListener('click', function(e) {
                    const languageMenu = document.querySelector('.language-dropdown');
                    const languageButton = document.getElementById('language-toggle');
                    
                    if (languageMenu && !languageMenu.contains(e.target) && 
                        languageButton && !languageButton.contains(e.target)) {
                        languageMenu.classList.add('hidden');
                    }
                });
            });
            
            // Cerrar men√∫ al hacer clic fuera
            document.addEventListener('click', function(e) {
                const languageMenu = document.querySelector('.language-dropdown');
                const languageButton = document.getElementById('language-toggle');
                
                if (!languageMenu.contains(e.target) && e.target !== languageButton && !languageButton.contains(e.target)) {
                    languageMenu.classList.add('hidden');
                }
            });
            
            // Actualizar flecha del bot√≥n cuando se muestra/oculta el men√∫
            const languageToggle = document.getElementById('language-toggle');
            const languageMenu = document.querySelector('.language-dropdown');
            const arrow = languageToggle.querySelector('svg');
            
            languageToggle.addEventListener('click', function() {
                if (languageMenu.classList.contains('hidden')) {
                    arrow.style.transform = 'rotate(180deg)';
                } else {
                    arrow.style.transform = 'rotate(0deg)';
                }
            });
            </script>
            
            <!-- Bot√≥n Men√∫ Hamburguesa -->
            <button id="menuToggle" class="text-orange-500 hover:text-orange-600 focus:outline-none">
                <i class="fas fa-bars text-2xl"></i>
            </button>
        </div>
    </div>
</header>

